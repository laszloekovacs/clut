{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Dart: terminal",
            "type": "dart-cli",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "Python: Terminal (integrated)",
            "type": "python",
